package application;

public class MainMenue {
	public void display() {
		System.out.println("Press 1 : Create");
		System.out.println("Press 2 : Display");
		System.out.println("Press 3 : Raise Salary");
		System.out.println("Press 4 : Exit");
	}
	public void displayCreate() {
		System.out.println("Press 1 : Programmer");
		System.out.println("Press 2 : Manager");
		System.out.println("Press 3 : Clerk ");
		System.out.println("Press 4 : Exit Create funcationality");
	}
}
